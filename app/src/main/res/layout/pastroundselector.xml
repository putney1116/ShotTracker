<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:id="@+id/pastroundselectorpage"
android:orientation="vertical"
android:background="@drawable/golfgreen"    		
>
	<RelativeLayout android:id="@+id/pastroundborderbox"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_margin="30dp"
    android:layout_centerInParent="true"
    android:background="#35697B"
    >
    	<RelativeLayout
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:id="@+id/listbox"
		android:background="#FFFFFF"   
		android:layout_margin="10dp"
    	android:layout_centerInParent="true" 		
		>
			<LinearLayout android:id="@+id/header"
			android:background="#ff347c12"
			android:layout_height="wrap_content"
			android:layout_width="fill_parent"
			>
				<TextView android:id="@+id/headeritem1"
				android:layout_height="fill_parent"
				android:layout_width="wrap_content"
				android:text="Course"
				android:width="175dip"
				android:textStyle="bold"
				android:gravity="center"
				/>
				<TextView android:id="@+id/headeritem2"
				android:layout_height="fill_parent"
				android:layout_width="wrap_content"
				android:text="Date"
				android:width="75dip"
				android:textStyle="bold"
				android:gravity="center"
				/>
			</LinearLayout>
			
			<View android:layout_width="fill_parent"
			android:layout_height="1dip"
			android:background="#000000" 
			android:id="@+id/divider"
			android:layout_below="@+id/header"
			/>
			
			<ListView android:id="@android:id/list"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"   
			android:layout_below="@+id/divider"
			>
			</ListView>
		</RelativeLayout>
	</RelativeLayout>	
</RelativeLayout>